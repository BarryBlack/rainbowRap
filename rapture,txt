        //<![CDATA[
        var dataForm = new VarienForm('login-form', true);

        jQuery('#login-form').on('submit', function() {
            if (!dataForm.validator.validate()) {
                jQuery('#login-form .validation-failed').each(function() {
                    jQuery(this).closest('.app-field').removeClass('valid');
                    jQuery(this).closest('.app-field').addClass('invalid');
                    jQuery(this).closest('.app-field').addClass('_changed');
                });
            }
        });
        jQuery('.app-field.required input').on('change', function() {
            jQuery(this).closest('.app-field').addClass('_changed');
            dataForm.validator.validate();
        });
        //]]>
    </script>
