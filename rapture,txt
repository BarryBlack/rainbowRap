            <div class="video-overlay">
<div class="block-subscribe-holder">
    <div class="block block-subscribe">
        <div class="block-title">
            <strong><span>We got<br />AFTER PARTIES<br />&amp; free SHIT</span></strong>
        </div>
        <form action="https://www.hufferstore.com/newsletter/subscriber/new/" method="post" id="newsletter-validate-detail">
            <div class="block-content">
                <div class="input-box">
                   <input type="email" autocapitalize="off" autocorrect="off" spellcheck="false" placeholder="Enter your email address here" name="email" id="newsletter" title="Enter your email address here" class="input-text required-entry validate-email" />
                </div>
                <div class="actions">
                    <button type="submit" title="JOIN NOW" class="button"><span><span>JOIN NOW</span></span></button>
                </div>
                <div class="success-messages"></div>
                <div class="error-messages"></div>
            </div>
        </form>
        <script type="text/javascript">
        //<![CDATA[
            var newsletterSubscriberFormDetail = new VarienForm('newsletter-validate-detail');

        jQuery('#newsletter-validate-detail').on('submit', function (e) {
            e.preventDefault();

            if (newsletterSubscriberFormDetail.validator.validate()) {
                var form = jQuery(this);
                jQuery('.newsletter_holder').css('opacity', '0.5');
                jQuery.ajax({
                    type: "POST",
                    url: form.attr('action'),
                    data: form.serialize(),
                    success: function (data) {
                        var successMessage = form.find('.success-messages');
                        var errorMessage = form.find('.error-messages');
                        successMessage.hide();
                        errorMessage.hide();
                        if (typeof data.messages != 'undefined') {
                            successMessage.html(data.messages.join("\n"));
                            successMessage.css('color', 'white').show(200);
                            setTimeout(function () {
                                successMessage.hide(200);
                            }, 10000);
                        }
                        if (typeof data.errors != 'undefined') {
                            errorMessage.html(data.errors.join("\n"));
                            errorMessage.css('color', 'white').show(200);
                            setTimeout(function () {
                                errorMessage.hide(200);
                            }, 10000);
                        }
                    },
                    complete: function (data) {
                        jQuery('.newsletter_holder').css('opacity', '1');
                    },
                    dataType: 'json'
                });
            }

            return false;
        });
        //]]>
        </script>
    </div>
