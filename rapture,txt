<script type="text/javascript">
    //<![CDATA[
    var dataForm = new VarienForm('register-form', false);
    jQuery(document).ready(function ($) {
        $("#register-form #password").change(function () {
            $("#register-form #confirmation").val($("#register-form #password").val());
        });
        jQuery('#register-form').submit(function (e) {
            e.preventDefault();
            var $this = $(this);
            var msgObj = $('#register-modal .msg');
            if (dataForm.validator.validate()) {
                jQuery.ajax({
                    url: $this.attr('action'),
                    dataType: 'json',
                    data: $this.serialize(),
                    type: 'POST',
                    beforeSend: function () {
                        $this.find('button[type="submit"]').attr('disabled', true);
                        msgObj.removeClass('error').removeClass('success');
                        msgObj.empty();
                    },
                    success: function (data) {
                        $this.find('button[type="submit"]').attr('disabled', false);
                        msgObj.addClass(data.status);
                        msgObj.html(data.message);
                        if(data.redirectUrl){
                            window.location.href = data.redirectUrl;
                        }
                    }
                });
            }
        });
    });
    //]]>
</script>        </div>

